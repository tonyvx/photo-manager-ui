{
  "name": "photo-manager-ui",
  "version": "0.1.0",
  "private": true,
  "main": "js/electron/app.js",
  "author": "tonyvx",
  "email": "anthony.valantra@gmail.com",
  "description": "Photo Manager 1.0",
  "homepage": "./",
  "dependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "leaflet": "^1.9.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-leaflet": "^4.2.1",
    "react-leaflet-vector-tile-layer": "^0.2.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "tsc": "tsc",
    "start": "react-scripts start",
    "build": "tsc;react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint --ext .js,.ts .",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
    "electron:start": "concurrently -k \"cross-env BROWSER=none npm start\" \"wait-on http://localhost:3000 && electronmon .\"",
    "electron": "npx tsc; npx webpack --mode production; npx electron .",
    "electron:package:mac": "npm run build && electron-builder -m -c.extraMetadata.main=build/app.js",
    "electron:package:win": "npm run build && electron-builder -w -c.extraMetadata.main=build/app.js",
    "electron:package:linux": "npm run build && electron-builder -l -c.extraMetadata.main=build/app.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      "last 1 electron version",
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 electron version",
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@types/electron": "^1.6.10",
    "@types/leaflet": "^1.9.6",
    "concurrently": "^8.2.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "electron": "^26.1.0",
    "electron-builder": "^24.6.4",
    "electronmon": "^2.0.2",
    "lint": "^0.8.19",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2",
    "wait-on": "^7.0.1",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4"
  },
  "build": {
    "appId": "org.tonyvx.photo-manager",
    "productName": "Photo Manager",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "electron"
    },
    "mac": {
      "target": "dmg"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "deb"
    }
  }
}
